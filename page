@Validated
public class MigrationProcessStatusResponse {
	
	@JsonProperty("migrationProcesses")
	@Valid
	private List<MigrationProcess> migrationProcesses = null;

	/**
	 * Get migrationProcesses
	 * 
	 * @return migrationProcesses
	 **/

	@Valid
	public List<MigrationProcess> getMigrationProcesses() {
		return migrationProcesses;
	}

	public void setMigrationProcesses(List<MigrationProcess> migrationProcesses) {
		this.migrationProcesses = migrationProcesses;
	}

}

--------------------------------------------------------------------------------------------

@Validated
public class MigrationProcess {
	
	@JsonProperty("migrationProcessId")
	private Integer migrationProcessId = null;

	@JsonProperty("migrationProcessType")
	private MigrationProcessTypeEnum migrationProcessType = null;

	@JsonProperty("migrationProcessStatus")
	private MigrationProcessStatusEnum migrationProcessStatus = null;

	public Integer getMigrationProcessId() {
		return migrationProcessId;
	}

	public void setMigrationProcessId(Integer migrationProcessId) {
		this.migrationProcessId = migrationProcessId;
	}

	public MigrationProcessTypeEnum getMigrationProcessType() {
		return migrationProcessType;
	}

	public void setMigrationProcessType(MigrationProcessTypeEnum migrationProcessType) {
		this.migrationProcessType = migrationProcessType;
	}

	public MigrationProcessStatusEnum getMigrationProcessStatus() {
		return migrationProcessStatus;
	}

	public void setMigrationProcessStatus(MigrationProcessStatusEnum migrationProcessStatus) {
		this.migrationProcessStatus = migrationProcessStatus;
	}
